<p id="notice"><%= notice %></p>

<h1>Listing Jigyoshos</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>会社CD</th>
      <th>会社略称</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jigyoshos.each do |jigyosho| %>
      <tr>
        <td><%= jigyosho.cd %></td>
        <td><%= jigyosho.ryakusho %></td>
        <td><%= link_to 'Show', jigyosho %></td>
        <td><%= link_to 'Edit', edit_jigyosho_path(jigyosho) %></td>
        <td><%= link_to 'Destroy', jigyosho, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jigyosho', new_jigyosho_path %>
